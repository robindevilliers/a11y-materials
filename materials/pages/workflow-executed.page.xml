<?schema version="2"?>
<page defnVersion="v1" id="intro" title="Workflow Executed" orientation="COLUMN">

    <jumbotron v="v1" orientation="COLUMN">
        <title size="LARGE" v="v1">
            <textual fontFlavour="WHITE">Workflow Executed</textual>
        </title>

        <paragraph v="v1">
            <textual>The <script v="v1" script=".title"/> workflow has been executed.
            </textual>
        </paragraph>

        <div justifyContent="END" v="v1">
            <accept-button id="accept-button" buttonFlavour="WHITE" margin="0.25em 0.5em" v="v1">
                <textual>Close</textual>
            </accept-button>
        </div>
    </jumbotron>
</page>